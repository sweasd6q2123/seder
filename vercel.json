{
  "version": 2,
  "builds": [],
  "routes": [
    {
      "src": "^/scripts/(.*)",
      "dest": "/scripts/$1"
    },
    {
      "src": "^/styles/(.*)",
      "dest": "/styles/$1"
    },
    {
      "src": "^/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}